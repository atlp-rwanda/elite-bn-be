{
  "name": "elite-bn",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "build": "babel ./src/index.js -d build",
    "start": "cross-env NODE_ENV=production babel-node ./src/index.js",
    "prod": "cross-env NODE_ENV=production nodemon --exec babel-node ./src/index.js",
    "dev": "cross-env NODE_ENV=development nodemon --exec babel-node ./src/index.js",
    "migration": "npx sequelize-cli db:migrate",
    "down": "npx sequelize-cli db:migrate:undo:all",
    "seed": "npx sequelize-cli db:seed:all",
    "coverage": "nyc npm run test | coveralls",
    "test": "cross-env NODE_ENV=test npm run migration && npm run seed && mocha --require @babel/register  --timeout 30000 --exit",
    "coveralls": "cross-env NODE_ENV=test nyc report --reporter=text-lcov | coveralls",
    "lint": "prettier --check .",
    "format": "prettier --write ."
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run format && git add -A ."
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/munyanezaarmel/elite-bn-be.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/atlp-rwanda/elite-bn-be.git"
  },
  "homepage": "https://github.com/atlp-rwanda/elite-bn-be.git",
  "dependencies": {
    "@babel/cli": "^7.17.10",
    "@babel/core": "^7.18.2",
    "@babel/eslint-parser": "^7.18.2",
    "@babel/node": "^7.17.10",
    "@babel/plugin-proposal-class-properties": "^7.17.12",
    "@babel/plugin-transform-runtime": "^7.18.0",
    "@babel/polyfill": "^7.12.1",
    "@babel/preset-env": "^7.18.0",
    "@babel/register": "^7.17.7",
    "@babel/runtime": "^7.18.0",
    "babel-eslint": "^10.1.0",
    "babel-node": "^0.0.1-security",
    "babel-plugin-add-module-exports": "^1.0.4",
    "babel-plugin-istanbul": "^6.1.1",
    "babel-register": "^6.26.0",
    "bcrypt": "^5.0.1",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.20.0",
    "c8": "^7.11.3",
    "chai": "^4.3.6",
    "chai-http": "^4.3.0",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "coverage": "^0.4.1",
    "cross-env": "^7.0.3",
    "dotenv": "^16.0.1",
    "eslint": "^8.16.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-node": "^4.1.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.25.3",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.30.0",
    "eslint-plugin-react-hooks": "^4.3.0",
    "express": "^4.18.1",
    "heroku-cli": "^7.0.9",
    "husky": "^8.0.1",
    "joi": "^17.6.0",
    "jsonwebtoken": "^8.5.1",
    "knex": "^2.1.0",
    "mocha": "^10.0.0",
    "mocha-lcov-reporter": "^1.3.0",
    "morgan": "^1.10.0",
    "node": "^17.7.2",
    "nodemon": "^2.0.16",
    "npm": "^8.12.1",
    "nyc": "^15.1.0",
    "package-lock.json": "^1.0.0",
    "passport": "^0.5.3",
    "pg": "^8.7.3",
    "pg-hstore": "^2.3.4",
    "pre-commit": "^1.2.2",
    "prettier": "^2.6.2",
    "sequelize": "^6.17.0",
    "sequelize-cli": "^6.4.1",
    "swagger-jsdoc": "^6.2.1",
    "swagger-ui-express": "^4.4.0"
  },
  "nyc": {
    "all": true,
    "include": [
      "src/**/*.js",
      "test/*.js"
    ],
    "reporter": [
      "html",
      "text",
      "text-lcov"
    ],
    "exclude": [
      "node_modules/*",
      "coverage/*"
    ],
    "sourceMap": true,
    "instrument": true,
    "sequelize": "^5.21.6",
    "sequelize-cli": "^5.5.1"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0"
  }
}
